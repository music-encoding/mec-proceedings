            <choice>
               <ref name="mei_model.divLike"/>
               <ref name="mei_model.textComponentLike"/>
            </choice>
            <zeroOrMore>
               <ref name="mei_model.milestoneLike.text"/>
            </zeroOrMore>
         </oneOrMore>
         <ref name="mei_att.common.attributes"/>
